<?xml version="1.0" encoding="utf-8"?>

<!-- Contains the privapp-permissions for the 1P sysui. -->
<permissions>
    <!-- 1P SysUi package. -->
    <privapp-permissions package="com.google.android.wearable.sysui">
        <permission name="android.permission.READ_FRAME_BUFFER"/>
        <permission name="android.permission.READ_PRIVILEGED_PHONE_STATE"/>
        <permission name="android.permission.WRITE_SECURE_SETTINGS"/>
        <permission name="android.permission.PACKAGE_USAGE_STATS"/>
        <permission name="com.google.android.clockwork.settings.WATCH_TOUCH" />
        <permission name="com.google.android.clockwork.systemui.BROADCAST_KEYGUARD_CHANGED"/>
        <permission name="com.google.android.wearable.permission.BIND_WEAR_CORE_PRIVILEGED_SERVICE"/>
        <permission name="com.google.android.clockwork.SIDEKICK_TIME_FORMAT_SET" />
        <permission name="com.android.clockwork.permission.GO_TO_SLEEP" />

        <permission name="android.permission.GET_TASKS" />
        <permission name="android.permission.REAL_GET_TASKS" />
        <permission name="android.permission.START_TASKS_FROM_RECENTS" />
        <permission name="android.permission.MANAGE_ACTIVITY_STACKS" />
        <permission name="android.permission.REMOVE_TASKS" />

        <permission name="com.google.android.wearable.permission.BIND_TILE_PROVIDER"/>
        <permission name="android.permission.CONTROL_REMOTE_APP_TRANSITION_ANIMATIONS" />
        <permission name="com.google.android.wearable.permission.BIND_WATCH_FACE_CONTROL" />

        <permission name="android.permission.OBSERVE_GRANT_REVOKE_PERMISSIONS" />
    </privapp-permissions>
</permissions>
